    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vaul-svelte": "^1.0.0-next.7",
    "vite": "^7.3.1"
  },
